<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.internalaudit.server.InternalAuditServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/internalaudit/greet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>com.internalaudit.server.FileUploadServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>/internalaudit/upload</url-pattern>
  </servlet-mapping>
  
  <!--  written new code of reference servlter class  Uploadservlet -->
  
    <servlet>
    <servlet-name>newUploadServlet</servlet-name>
    <servlet-class>com.internalaudit.server.UploadServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>newUploadServlet</servlet-name>
    <url-pattern>/internalaudit/newUpload</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>AuditNotificationUploadServlet</servlet-name>
    <servlet-class>com.internalaudit.server.AuditNotificationUploadServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>AuditNotificationUploadServlet</servlet-name>
    <url-pattern>/internalaudit/AuditNotificationUpload</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>EngagementPlanningUploadServlet</servlet-name>
    <servlet-class>com.internalaudit.server.EngagementPlanningUploadServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>EngagementPlanningUploadServlet</servlet-name>
    <url-pattern>/internalaudit/EngagementPlanningUpload</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
    <servlet-name>AuditWorkProgramUploadServlet</servlet-name>
    <servlet-class>com.internalaudit.server.AuditWorkProgramUploadServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>AuditWorkProgramUploadServlet</servlet-name>
    <url-pattern>/internalaudit/AuditWorkProgramUpload</url-pattern>
  </servlet-mapping>
  
      <servlet>
    <servlet-name>EmailAttachmentUploadServlet</servlet-name>
    <servlet-class>com.internalaudit.server.EmailAttachmentUploadServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>EmailAttachmentUploadServlet</servlet-name>
    <url-pattern>/internalaudit/EmailAttachmentUpload</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>InternalAudit.html</welcome-file>
  </welcome-file-list>
  
  
  

</web-app>
