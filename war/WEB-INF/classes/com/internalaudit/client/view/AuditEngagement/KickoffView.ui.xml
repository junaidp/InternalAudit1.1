<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:B="urn:import:com.google.gwt.dev.shell.BrowserChannel" xmlns:A="urn:import:com.internalaudit.client.view.AuditEngagement">
	<ui:style>
	.lbl{
	margin-left: 50px;
}
.panel{
	
	margin-top: 5px;
}
	.heading{
	font-size: 18px;
}
	</ui:style>
	<g:HTMLPanel>

		<A:LabelHeading ui:field="jobName" addStyleNames="{style.heading}"> Loading.. </A:LabelHeading>
		
	<g:VerticalPanel addStyleNames="{style.panel}">
		<g:HorizontalPanel>
		 	<g:VerticalPanel>
		    <g:Label styleName="labelTitle" width="200px" addStyleNames=" w3-light-blue" >Planned Start Date</g:Label> 
			</g:VerticalPanel>
		 	<g:VerticalPanel>
		 	<g:Label  styleName="labelValue" width="250px"  ui:field="startDate"  > Loading..</g:Label>
		 	</g:VerticalPanel>
		 	<g:VerticalPanel>
			<g:Label styleName="labelTitle" width="200px"  addStyleNames=" w3-light-blue {style.lbl}"  >Planned End Dates</g:Label>
			</g:VerticalPanel>
			<g:VerticalPanel>
			 <g:Label  styleName="labelValue" width="200px"  ui:field="endDate"> Loading..</g:Label>
			</g:VerticalPanel>
		</g:HorizontalPanel>	
		<g:HorizontalPanel addStyleNames="{style.panel}">
			
		</g:HorizontalPanel>
		
		<g:HorizontalPanel addStyleNames="{style.panel}">
			<g:VerticalPanel>
				<g:Label styleName="labelTitle" width="200px"  addStyleNames=" w3-light-blue" >Auditable Unit</g:Label> 
		    </g:VerticalPanel>
			<g:VerticalPanel>
			    <g:Label text="Loading.."  width="200px" ui:field="lblAuditableUnit" addStyleNames="{style.lbl}" ></g:Label>
			</g:VerticalPanel>
		</g:HorizontalPanel>
			
		<g:HorizontalPanel addStyleNames="{style.panel}"  >
			<g:VerticalPanel>
				 <g:Label styleName="labelTitle" width="200px"  addStyleNames=" w3-light-blue">Process</g:Label> 
			</g:VerticalPanel>
			<g:VerticalPanel>
 				 <g:Label text="Loading.."  width="200px" ui:field="lblProcess" addStyleNames="{style.lbl}"></g:Label>
			</g:VerticalPanel>
				<g:VerticalPanel>
				<g:Label styleName="labelTitle" width="200px"  addStyleNames=" w3-light-blue {style.lbl}" >SubProcess</g:Label>
			</g:VerticalPanel>
			<g:VerticalPanel>
			    <g:Label text="Loading.." width="200px"  ui:field="lblSubProcess" addStyleNames="{style.lbl}"></g:Label>
			</g:VerticalPanel>
		</g:HorizontalPanel>
			
		<g:HorizontalPanel addStyleNames="{style.panel}" >
		
		</g:HorizontalPanel>
			
		<g:HorizontalPanel addStyleNames="{style.panel}" >
			<g:VerticalPanel>
				<g:Label styleName="labelTitle" width="200px"  addStyleNames=" w3-light-blue" >Job Type</g:Label> 
			</g:VerticalPanel>
			<g:VerticalPanel>
			     <g:Label ui:field="lblJobType" width="200px"  text="Assurance" addStyleNames="{style.lbl}"></g:Label> 
			</g:VerticalPanel>
		</g:HorizontalPanel>
		
			
				
<!-- 			<g:HorizontalPanel styleName="statusRow"> -->
				
<!-- 				<g:Label styleName="labelTitle">Job Incharge</g:Label> <g:Label  styleName="labelValue" ui:field="incharge"> Incharge Name</g:Label> -->
				
<!-- 			</g:HorizontalPanel>		 -->
		
		</g:VerticalPanel>
		
		<g:VerticalPanel ui:field="statusPanel">
			
		</g:VerticalPanel>

		
		<g:VerticalPanel ui:field="exPanel">
			
		</g:VerticalPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 