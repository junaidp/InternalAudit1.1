<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"  xmlns:m="urn:import:com.sencha.gxt.widget.core.client.menu" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:b="urn:import:com.sun.javafx.scene.control.behavior">
	<ui:style>
	.padding{
	padding-top: 15px;
}
/*.button{
	margin-left: 50px;
}*/
	</ui:style>
	<g:HTMLPanel>
	<!--<g:VerticalPanel addStyleNames="w3-panel w3-pale-green"> -->
	<g:VerticalPanel>
	
	<g:HorizontalPanel>
	<g:Label styleName="gwt-Label auditengLabel" width="85px" text="Task:"></g:Label>
	<g:TextBox text="some text"></g:TextBox>
	</g:HorizontalPanel>
	
	<g:HorizontalPanel>
	<g:Label styleName="gwt-Label auditengLabel" width="85px">Due Date:</g:Label>
	<d:DateBox ui:field="dueDate"></d:DateBox>
	</g:HorizontalPanel>
	
	<g:HorizontalPanel>
	<g:Label styleName="gwt-Label auditengLabel" width="85px">Assigned To: </g:Label>
	<g:ListBox ui:field="listBoxAssignedTo" ></g:ListBox>
	</g:HorizontalPanel>
	
	<g:HorizontalPanel>
	<g:Label styleName="gwt-Label auditengLabel" width="85px">Jobs:</g:Label>
	<g:ListBox ui:field="listBoxJobs" >	</g:ListBox>
	</g:HorizontalPanel>
	
	
	<g:Label styleName="gwt-Label auditengLabel" width="85px">Description:</g:Label>
	<g:HorizontalPanel>
	<g:TextArea ui:field="txtBoxDescription"  width="350px" height="100px">Enter Text here</g:TextArea>
	</g:HorizontalPanel>
	
	<g:VerticalPanel width="100%" styleName="w3-border" ui:field="panelAttachment">
	
	</g:VerticalPanel>
	<g:HorizontalPanel>
	<g:VerticalPanel>
	<g:Button ui:field="btnSave" addStyleNames="buttonDesign w3-hover-blue">Save</g:Button>
	</g:VerticalPanel>
	<g:VerticalPanel>
	<g:Button ui:field="btnCancel" addStyleNames="buttonDesign w3-hover-blue">Cancel</g:Button>
	</g:VerticalPanel>
	</g:HorizontalPanel>
	</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 